% 设置随机数种子以确保可复现性
rng(1); 

% 初始化一个矩阵来存储结果
randomNumbers = zeros(50, 8);

% 循环生成50组随机数
for i = 1:50
    % 生成0.1到0.7之间8个不重复的索引
    idx = (randperm(60, 8)-1)/100; % 生成8个（0~59）/100的数字
    % 将索引映射到0.1到0.7的范围内，并四舍五入到两位小数
    randomNumbers(i,:) = idx + 0.1;
end

% 显示前几组随机数作为示例
disp(randomNumbers(1:5,:));